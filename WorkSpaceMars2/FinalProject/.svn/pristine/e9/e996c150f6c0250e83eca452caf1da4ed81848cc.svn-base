<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!-- 列举变更记录的Action -->
	<package name="_change" namespace="/change" extends="struts-default">
		<action name="change_*" class="com.kms.action.ChangeListAction" method="{1}">
			<result name="listChange">/NewAdmView/jumpPage/changeManager.jsp</result>
			<result name="listChangeByKeyWord">/change/changeList.jsp</result>
			<result name="error"></result>
		</action>
	</package>
	
</struts>
