package com.kms.po;

/**用于封装在职员工基本信息：
 * 员工编号，姓名，性别，民族，身份证号，联系电话，学历，户籍
 * @author 张金海
 *
 */
public class WorkerSalary {
	private String jid;
	private String jnm;
	private int jage;
	private String jposi;
	private String jdept;
	private int jsal;
	public String getJid() {
		return jid;
	}
	public void setJid(String jid) {
		this.jid = jid;
	}
	public String getJnm() {
		return jnm;
	}
	public void setJnm(String jnm) {
		this.jnm = jnm;
	}
	public int getJage() {
		return jage;
	}
	public void setJage(int jage) {
		this.jage = jage;
	}
	public String getJposi() {
		return jposi;
	}
	public void setJposi(String jposi) {
		this.jposi = jposi;
	}
	public String getJdept() {
		return jdept;
	}
	public void setJdept(String jdept) {
		this.jdept = jdept;
	}
	public int getJsal() {
		return jsal;
	}
	public void setJsal(int jsal) {
		this.jsal = jsal;
	}
	
}
