package com.kms.po;

import java.util.Date;

/**用于封装试用期员工表(probationinfo)中的信息，
 * 对外提供接口函数供数据交换
 * @author 张金海
 *
 */
public class Probation {
	private String pid;
	private String pnm;
	private String psex;
	private int page;
	private String pna;
	private String piid;
	private String ptel;
	private String pedu;
	private String ploc;
	private Date pst;
	private Date pend;
	private String ppos;
	private String pdept;
	public String getPid() {
		return pid;
	}
	public void setPid(String pid) {
		this.pid = pid;
	}
	public String getPnm() {
		return pnm;
	}
	public void setPnm(String pnm) {
		this.pnm = pnm;
	}
	public String getPsex() {
		return psex;
	}
	public void setPsex(String psex) {
		this.psex = psex;
	}
	public int getPage() {
		return page;
	}
	public void setPage(int page) {
		this.page = page;
	}
	public String getPna() {
		return pna;
	}
	public void setPna(String pna) {
		this.pna = pna;
	}
	public String getPiid() {
		return piid;
	}
	public void setPiid(String piid) {
		this.piid = piid;
	}
	public String getPtel() {
		return ptel;
	}
	public void setPtel(String ptel) {
		this.ptel = ptel;
	}
	public String getPedu() {
		return pedu;
	}
	public void setPedu(String pedu) {
		this.pedu = pedu;
	}
	public String getPloc() {
		return ploc;
	}
	public void setPloc(String ploc) {
		this.ploc = ploc;
	}
	public Date getPst() {
		return pst;
	}
	public void setPst(Date pst) {
		this.pst = pst;
	}
	public Date getPend() {
		return pend;
	}
	public void setPend(Date pend) {
		this.pend = pend;
	}
	public String getPpos() {
		return ppos;
	}
	public void setPpos(String ppos) {
		this.ppos = ppos;
	}
	public String getPdept() {
		return pdept;
	}
	public void setPdept(String pdept) {
		this.pdept = pdept;
	}
}
