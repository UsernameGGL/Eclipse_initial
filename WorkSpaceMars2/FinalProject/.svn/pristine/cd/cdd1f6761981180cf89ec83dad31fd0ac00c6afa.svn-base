package com.kms.po;

import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;

/**封装记录变更表的数据，
 * 对外提供接口函数以便数据交换
 * @author 刘嘉赓
 *
 */
public class Change {
	private int rid;
	private String cnm;
	private String cop;
	private String cold;
	private String cnew;
	private int cosal;
	private int cnsal;
	private Date cdate;
	
	public int getRid() {
		return rid;
	}
	public void setRid(int rid) {
		this.rid = rid;
	}
	public String getCnm() {
		return cnm;
	}
	public void setCnm(String cnm) {
		this.cnm = cnm;
	}
	public String getCop() {
		return cop;
	}
	public void setCop(String cop) {
		this.cop = cop;
	}
	public String getCold() {
		return cold;
	}
	public void setCold(String cold) {
		this.cold = cold;
	}
	public String getCnew() {
		return cnew;
	}
	public void setCnew(String cnew) {
		this.cnew = cnew;
	}
	public int getCosal() {
		return cosal;
	}
	public void setCosal(int cosal) {
		this.cosal = cosal;
	}
	public int getCnsal() {
		return cnsal;
	}
	public void setCnsal(int cnsal) {
		this.cnsal = cnsal;
	}
	public Date getCdate() {
		return cdate;
	}
	public void setCdate() {
		this.cdate = new Date();
	}
	public void setCdate(Date cdate) {
		this.cdate = cdate;
	}
}
