package com.kms.po;

import java.io.Serializable;

/** 用于封装Position岗位信息，对外提供函数接口用于数据交换
 * private int pid，岗位编号
 * private String pnm，岗位名称
 * private int pnum，岗位员工总数
 * @author 刘嘉赓
 */
public class Position implements Serializable{

	private static final long serialVersionUID = 7308510365526140500L;
	private int pid;			//岗位编号
	private String pnm;			//岗位名称
	private int pnum;			//岗位正式员工总数
	
	public int getPid() {
		return pid;
	}
	public void setPid(int pid) {
		this.pid = pid;
	}
	public String getPnm() {
		return pnm;
	}
	public void setPnm(String pnm) {
		this.pnm = pnm;
	}
	public int getPnum() {
		return pnum;
	}
	public void setPnum(int pnum) {
		this.pnum = pnum;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
}
